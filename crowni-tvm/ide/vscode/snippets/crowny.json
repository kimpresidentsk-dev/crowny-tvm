{
  "함수 정의": {
    "prefix": ["함수", "func"],
    "body": [
      "함수 ${1:이름} {",
      "    ${2:; 내용}",
      "}",
      ""
    ],
    "description": "함수 정의"
  },
  "변수 선언": {
    "prefix": ["변수", "var"],
    "body": ["변수 ${1:이름} = ${2:값}"],
    "description": "변수 선언"
  },
  "3진 분기": {
    "prefix": ["만약", "if3"],
    "body": [
      "만약 {",
      "    ${1:; P(참) 블록}",
      "} 보류 {",
      "    ${2:; O(모름) 블록}",
      "} 아니면 {",
      "    ${3:; T(거짓) 블록}",
      "}",
      ""
    ],
    "description": "3진 분기 (만약/보류/아니면)"
  },
  "반복": {
    "prefix": ["반복", "loop"],
    "body": [
      "반복 ${1:횟수} {",
      "    ${2:; 반복 내용}",
      "}",
      ""
    ],
    "description": "반복문"
  },
  "LLM 호출": {
    "prefix": ["질문해", "ask", "llm"],
    "body": [
      "질문해 \"${1:프롬프트}\"",
      "보여줘"
    ],
    "description": "LLM 호출"
  },
  "기본 프로그램": {
    "prefix": ["crowny", "새파일"],
    "body": [
      "; Crowny WebScript — ${1:프로그램 이름}",
      "; 날짜: ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
      "",
      "${2:; 코드}",
      "",
      "끝"
    ],
    "description": "새 Crowny 프로그램 템플릿"
  },
  "계산": {
    "prefix": ["계산", "calc"],
    "body": [
      "값 ${1:10}",
      "값 ${2:20}",
      "${3|더,빼,곱,나눠|}",
      "보여줘"
    ],
    "description": "간단한 계산"
  },
  "AI 판단": {
    "prefix": ["판단", "decide"],
    "body": [
      "; AI 3진 판단",
      "질문해 \"${1:질문}\"",
      "만약 {",
      "    ${2:; 합격/승인}",
      "} 보류 {",
      "    ${3:; 추가 심사}",
      "} 아니면 {",
      "    ${4:; 거부}",
      "}",
      ""
    ],
    "description": "AI 기반 3진 판단 패턴"
  }
}

{
  "name": "@crowny/sdk",
  "version": "1.0.0",
  "description": "Crowny 균형3진 플랫폼 JavaScript SDK — CAR.submit() + CTP + 3진 합의",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest run",
    "dev": "tsup src/index.ts --watch"
  },
  "keywords": ["crowny", "ternary", "trit", "balanced-ternary", "ai-consensus", "ctp"],
  "author": "KPS (Han Seon)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kimpresidentsk-dev/crowny-tvm"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
